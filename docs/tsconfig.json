{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "@site/*": ["./*"],
      "@assets/*": ["/src/assets/*"],
      "@react/*": ["/src/components/react/*"],
      "@astro/*": ["/src/components/astro/*"]
    }
  }
}
