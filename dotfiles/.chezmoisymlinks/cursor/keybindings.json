[
  {
    "key": "enter",
    "command": "renameFile",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus && isWindows"
  },
  {
    "key": "f2",
    "command": "-renameFile",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus && isWindows"
  },
  {
    "key": "ctrl+shift+u",
    "command": "editor.action.transformToUppercase",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+t",
    "command": "typescript.restartTsServer",
    "when": "isWindows"
  },
  {
    "key": "ctrl+shift+r",
    "command": "workbench.action.reloadWindow",
    "when": "isWindows"
  },
]
